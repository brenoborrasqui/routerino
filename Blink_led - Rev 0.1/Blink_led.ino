void setup() {
  Serial.begin(9600);
  SPI_MasterBegin();
  ENC28J60_BlinkLEDs(500, 1);
  ENC28J60_BlinkLEDs(500, 2);
  ENC28J60_Reset(1);
  ENC28J60_Reset(2);

  Serial.print("Version1: 0x");
  Serial.println(ENC28J60_Revision(1), HEX);

  Serial.print("Version2: 0x");
  Serial.println(ENC28J60_Revision(2), HEX);
}

